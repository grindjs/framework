{
  "compilerOptions": {
    "module": "commonjs",
    "noImplicitAny": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "outDir": "lib",
    "target": "es2018",
    "forceConsistentCasingInFileNames": true,
    "types": ["node"]
  },

  "exclude": ["node_modules", "dist"]
}
