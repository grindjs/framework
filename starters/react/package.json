{
  "name": "grind-example-react",
  "version": "0.8.0-beta.0",
  "private": true,
  "scripts": {
    "build": "bin/build",
    "lint-js": "eslint app boot public",
    "lint-scss": "sass-lint --verbose --max-warnings=0 --no-exit"
  },
  "dependencies": {
    "@khrrsn/react-conductor": "^1.0.8",
    "classnames": "^2.2.6",
    "grind-core-frontend": "^0.8.0-beta.4",
    "grind-db": "^0.8.0-beta.1",
    "grind-orm": "^0.8.0-beta.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "sqlite3": "^4.0.9"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "autoprefixer": "^9.6.1",
    "babel-eslint": "^10.0.2",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-transform-isnil": "^1.0.1",
    "babel-preset-grind": "^0.8.0-beta.4",
    "clean-css": "^4.2.1",
    "eslint": "^6.1.0",
    "eslint-config-grind": "^2.3.0",
    "eslint-plugin-import-auto-name": "^1.1.0",
    "eslint-plugin-react": "^7.14.3",
    "grind-react-dev": "^0.8.0-beta.1",
    "node-sass": "^4.12.0",
    "normalize.css": "^8.0.1",
    "postcss": "^7.0.17",
    "rollup": "^1.17.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-replace": "^2.2.0",
    "sass-lint": "^1.13.1",
    "svgo": "^1.3.0",
    "uglify-es": "^3.3.9",
    "ws": "^7.1.1"
  },
  "engines": {
    "node": ">=10.0"
  },
  "babel": {
    "presets": [
      "grind"
    ],
    "plugins": [
      [
        "module-resolver",
        {
          "alias": {
            "App": "./app",
            "Boot": "./boot"
          }
        }
      ]
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "plugins": [
      "import-auto-name"
    ],
    "env": {
      "node": true
    },
    "extends": "grind",
    "parserOptions": {
      "sourceType": "module"
    },
    "globals": {
      "ValidationError": false
    }
  }
}
