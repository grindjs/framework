{
  "name": "grind-toolkit",
  "version": "0.8.0-beta.8",
  "description": "CLI Tookit for Grind",
  "keywords": [
    "grindjs",
    "grind-framework"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/grindjs/toolkit.git"
  },
  "license": "MIT",
  "bin": {
    "grind": "./lib/bin/cli"
  },
  "scripts": {
    "lint": "eslint app boot",
    "prepack": "bin/build"
  },
  "babel": {
    "presets": [
      [
        "grind",
        {
          "import-auto-name": {
            "autoresolve": true
          }
        }
      ]
    ]
  },
  "eslintConfig": {
    "env": {
      "node": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "sourceType": "module"
    },
    "plugins": [
      "import-auto-name"
    ],
    "extends": "grind",
    "globals": {
      "ValidationError": false
    }
  },
  "dependencies": {
    "as-type": "^1.0.0",
    "fetchit": "^2.1.0",
    "grind-cli": "^0.8.0-beta.4",
    "grind-framework": "^0.8.0-beta.2",
    "grind-stone": "^0.8.0-beta.9",
    "grind-view": "^0.8.0-beta.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/register": "^7.4.4",
    "babel-eslint": "^10.0.1",
    "babel-preset-grind": "^0.8.0-beta.4",
    "eslint": "^5.16.0",
    "eslint-config-grind": "^2.3.0",
    "eslint-plugin-import-auto-name": "^1.0.3"
  },
  "engines": {
    "node": ">=8.7",
    "npm": ">=3.0"
  },
  "preferGlobal": true
}
