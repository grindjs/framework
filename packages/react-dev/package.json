{
  "name": "grind-react-dev",
  "version": "0.8.0-beta.1",
  "description": "Utility package for development with Grind and React",
  "repository": {
    "type": "git",
    "url": "https://github.com/grindjs/react-dev.git"
  },
  "license": "MIT",
  "author": "Shaun Harrison (@shnhrrsn)",
  "main": "lib/index.js",
  "browser": "dist/react-dev.min.js",
  "scripts": {
    "lint": "eslint src",
    "prepare": "bin/build"
  },
  "babel": {
    "presets": [
      [
        "grind",
        {
          "import-auto-name": {
            "autoresolve": true
          }
        }
      ]
    ]
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "sourceType": "module"
    },
    "plugins": [
      "import-auto-name"
    ],
    "extends": "grind"
  },
  "dependencies": {
    "react-dev-utils": "^9.0.1",
    "react-error-overlay": "^5.1.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "babel-eslint": "^10.0.2",
    "babel-preset-grind": "^0.8.0-beta.4",
    "eslint": "^6.1.0",
    "eslint-config-grind": "^2.3.0",
    "eslint-plugin-import-auto-name": "^1.1.0",
    "rimraf": "^2.6.3",
    "rollup": "^1.17.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "uglify-es": "^3.3.9"
  },
  "peerDependencies": {
    "grind-assets": "^0.8.0-beta.14",
    "grind-framework": "^0.8.0-beta.4"
  },
  "engines": {
    "node": ">=10.13"
  }
}
