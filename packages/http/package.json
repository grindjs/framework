{
  "name": "grind-http",
  "version": "0.8.0-beta.4",
  "description": "HTTP kernel for Grind, powered by Express",
  "keywords": [
    "grindjs",
    "grind-framework",
    "grind-kernel",
    "express",
    "http"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/grindjs/framework.git"
  },
  "license": "MIT",
  "author": "Shaun Harrison (@shnhrrsn)",
  "main": "lib/index.js",
  "scripts": {
    "prepack": "bin/build",
    "test": "bin/ava --verbose"
  },
  "babel": {
    "presets": [
      [
        "grind",
        {
          "import-auto-name": {
            "autoresolve": true
          }
        }
      ]
    ]
  },
  "ava": {
    "require": [
      "@babel/register"
    ]
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.14.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/register": "^7.0.0",
    "ava": "^1.0.0-rc.1",
    "babel-preset-grind": "^0.8.0-beta.3",
    "chokidar": "^2.0.4",
    "compression": "^1.7.3",
    "cookie-parser": "^1.4.3",
    "express-flash": "^0.0.2",
    "express-session": "^1.15.6",
    "flash": "^1.1.0",
    "grind-cli": "^0.8.0-beta.1",
    "grind-db": "^0.8.0-beta.1",
    "grind-framework": "^0.8.0-beta.1",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.5",
    "sqlite3": "^4.0.3"
  },
  "peerDependencies": {
    "grind-framework": "^0.8.0-beta.1"
  },
  "engines": {
    "node": ">=10.13"
  },
  "grind": {
    "commands": [
      "./lib/Commands/MakeControllerCommand.js"
    ],
    "providers": []
  }
}
